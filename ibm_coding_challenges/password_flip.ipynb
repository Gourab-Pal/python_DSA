   "source": [
    "def flipCounts(pwd):\n",
    "    count = 0\n",
    "    for i in range(0, len(pwd)-1, 2):\n",
    "        if pwd[i] != pwd[i+1]:\n",
    "            count = count + 1\n",
    "    return count"
   ]
  },
