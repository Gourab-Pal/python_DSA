    "def transform(arr):\n",
    "    n = len(arr)\n",
    "    for i in range(n):\n",
    "        if arr[i]%2==1:\n",
    "            arr[i] = arr[i] + 3\n",
    "        else:\n",
    "            arr[i] = arr[i] - 3\n",
    "    return arr\n",
    "\n",
    "transform([3,4,9])"
   ]
  }
